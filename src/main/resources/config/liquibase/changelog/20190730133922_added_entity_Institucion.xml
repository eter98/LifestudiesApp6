            </column>

            <column name="programa_negocios" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="preparacion_examen" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="programa_academico" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="descuento" type="integer">
                <constraints nullable="true" />
            </column>

            <column name="inscripcion" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="materiales" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="seguro_medico" type="varchar(255)">
                <constraints nullable="true" />
