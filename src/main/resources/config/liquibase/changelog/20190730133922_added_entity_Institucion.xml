            <column name="skype" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="telefono" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="estatus" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="tipo_programas" type="varchar(255)">
