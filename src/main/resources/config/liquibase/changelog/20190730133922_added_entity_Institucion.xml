            <column name="alojamiento_sencillo" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="alojamiento_doble" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="transporte_aeropuerto" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="tipo_curso" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="temporada_alta" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="temporada_baja" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="fecha_inicial" type="date">
                <constraints nullable="true" />
            </column>

            <column name="horarios" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="instalaciones" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="nacionalidad" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="logo" type="longblob">
                <constraints nullable="true" />
            </column>
            <column name="logo_content_type" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="foto_1" type="longblob">
                <constraints nullable="true" />
            </column>
            <column name="foto_1_content_type" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="foto_2" type="longblob">
                <constraints nullable="true" />
            </column>
            <column name="foto_2_content_type" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="foto_3" type="longblob">
                <constraints nullable="true" />
            </column>
            <column name="foto_3_content_type" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="ciudad_id" type="bigint">
                <constraints nullable="true" />
            </column>

            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here, do not remove-->
