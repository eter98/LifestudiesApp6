<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Cotizacion.
    -->
    <changeSet id="20190730133925-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="usario_id"
                                 baseTableName="cotizacion"
                                 constraintName="fk_cotizacion_usario_id"
                                 referencedColumnNames="id"
                                 referencedTableName="perfil_usuario"/>

        <addForeignKeyConstraint baseColumnNames="curso_id"
                                 baseTableName="cotizacion"
                                 constraintName="fk_cotizacion_curso_id"
                                 referencedColumnNames="id"
                                 referencedTableName="programas"/>

    </changeSet>
</databaseChangeLog>
